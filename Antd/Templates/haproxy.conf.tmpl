global
    daemon
    maxconn 256

defaults
    mode http
    timeout connect 5000ms
    timeout client 50000ms
    timeout server 50000ms

frontend http-in
    bind 10.1.122.1:80
    default_backend servers

backend servers
    server server0 node00:8086 maxconn 32
    server server1 node01:8086 maxconn 32
