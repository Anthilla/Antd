<legend id="System">System Users</legend>
<input type="text" class="search-field" style="position: relative; margin-bottom: 15px; height: 30px; width: 100%; padding-left: 5px;" placeholder="Search">
<table class="table searchable">
    <thead>
        <tr>
            <th class="text-left">user</th>
            <th class="text-left">group id</th>
            <th class="text-left">home</th>
            <th class="text-left">shell</th>
            <th class="text-left">info</th>
            <th class="text-left" style="width: 20%;"></th>
        </tr>
    </thead>
    <tbody id="SysctlBodyTable">
        @foreach (antdlib.Users.UserModel user in Model.SystemUsers) {
            <tr>
                <td style="text-align: center;">@user.UID - @user.Alias </td>
                <td style="text-align: center;">@user.GroupID </td>
                <td style="text-align: center;">@user.HomeDirectory </td>
                <td style="text-align: center;">@user.LoginShell </td>
                <td style="text-align: center;">@user.Info </td>
                <td style="text-align: center;"><input type="hidden" value="@user.Password.Result" /> </td>
            </tr>
        }
    </tbody>
</table>