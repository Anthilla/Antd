<legend id="List">App List</legend>

@foreach (antdlib.Apps.Management.AppInfo app in antdlib.Apps.Management.DetectApps()) {
    <table class="table">
        <tbody>
            <tr><td colspan="2"><label>@app.Name</label></td></tr>
            @foreach (KeyValuePair<string, string> kvp in app.Values) {
                <tr>
                    <td><label>@kvp.Key</label></td>
                    <td><p>@kvp.Value</p></td>
                </tr>
            }
        </tbody>
    </table>
}
