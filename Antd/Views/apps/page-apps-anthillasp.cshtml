</a>

<script type="text/javascript">
    $('#SetAnthillaSP').click(function () {
        jQuery.support.cors = true;
        $.ajax({
            url: '/apps/launch/',
            type: 'GET',
            contentType: "application/json;charset=utf-8",
            success: function (data) {
                alert(data);
                return false;
            }
        });
    });

    $('#StartAnthillaSP').click(function () {
        jQuery.support.cors = true;
        $.ajax({
            url: '/apps/start/sp/',
            type: 'GET',
            contentType: "application/json;charset=utf-8",
            success: function (data) {
                alert(data);
                return false;
            }
        });
    });

    $('#StartAnthillaServer').click(function () {
        jQuery.support.cors = true;
        $.ajax({
            url: '/apps/start/server/',
            type: 'GET',
            contentType: "application/json;charset=utf-8",
            success: function (data) {
                alert(data);
                return false;
            }
        });
    });
</script>
