<legend id="Disks">Disks</legend>
<table class="table">
    <tbody>
        @foreach (Antd.Storage.Volumes.Block blk in Antd.Storage.Volumes.Blkid()) { 
            <tr>
                <td class="right">@blk.Name</td>
                <td class="right">@blk.Attributes</td>
            </tr>
        }
    </tbody>
</table>

