<legend id="InfoDmidecode">Dmidecode</legend>
@if (Model.DMIDECODE != null) {
    @if (Model.DMIDECODE.outputTable != null) {
        <table id="Output" class="table">
            <tbody>
                @foreach (string str in Model.DMIDECODE.outputTable) {
                    <tr>
                        <td class="right">@str</td>
                    </tr>
                }
            </tbody>
        </table>
    }
    <br />
    @if (Model.DMIDECODE.errorTable != null) {
        <table id="Error" class="table">
            <tbody>
                @foreach (string str in Model.DMIDECODE.errorTable) {
                    <tr>
                        <td class="right">@str</td>
                    </tr>
                }
            </tbody>
        </table>
    }
}