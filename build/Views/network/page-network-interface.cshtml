<legend id="Interface">Interface</legend>
@foreach (Antd.Models.NetworkInterfaceModel netInt in Model.Interfaces) {
    <table class="table hovered">
        <tbody>
            <tr>
                <td style="background-color: #D8EC9C;">
                    <label class="fg-black">Application Layer</label>
                </td>
                <td>
                    <p>&nbsp</p>
                </td>
            </tr>
            <tr>
                <td style="background-color: #D8EC9C;">
                    <label class="fg-black">Presentation Layer</label>
                </td>
                <td>
                    <p>&nbsp</p>
                </td>
            </tr>
            <tr>
                <td style="background-color: #D8EC9C;">
                    <label class="fg-black">Session Layer</label>
                </td>
                <td>
                    <p>&nbsp</p>
                </td>
            </tr>
            <tr>
                <td style="background-color: #E7ED9C;">
                    <label class="fg-black">Transport Layer</label>
                </td>
                <td>
                    <p>&nbsp</p>
                </td>
            </tr>
            <tr>
                <td style="background-color: #EDDC9C;">
                    <label class="fg-black">Network Layer</label>
                </td>
                <td>
                    <p>@netInt.Network</p>
                </td>
            </tr>
            <tr>
                <td style="background-color: #E9C189;">
                    <label class="fg-black">Data link Layer</label>
                </td>
                <td>
                    <p>@netInt.Datalink</p>
                </td>
            </tr>
            <tr>
                <td style="background-color: #E9988A;">
                    <label class="fg-black">Physical Layer</label>
                </td>
                <td>
                    <p>@netInt.Physical</p>
                </td>
            </tr>
        </tbody>
    </table>
}