<legend id="Main">Main</legend>
<table id="log" class="table">
    <thead>
        <tr>
            <th class="text-left">Timestamp</th>
            <th class="text-left">Action</th>
            <th class="text-left">File</th>
            <th class="text-left">Old File</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Antd.Log.LogModel log in Model.LOGS) {
            <tr>
                <td class="right">
                    <p>@log.time</p>
                </td>
                <td class="right">
                    <p>@log.mode</p>
                </td>
                <td class="right">
                    <p>@log.file</p>
                </td>
                <td class="right">
                    @if (log.oldfile != "") {
                        <p>@log.oldfile</p>
                    }
                    else {
                        <p>&nbsp</p>
                    }
                </td>
            </tr>
        }
    </tbody>
</table>
