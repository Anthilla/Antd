<script language=javascript>
var WO=window.opener;
if (WO!=null) {
  if (!WO.closed) {
     if (WO.parent.body.document.title=="QoS List") {
       //WO.parent.body.location.reload(true);
       WO.parent.body.location="kerbynet?Section=QoS&STk=<xvar _STk>&Action=List&Type="+WO.parent.body.document.data.TYPE.value;
     } else {
       if ("<xvar +qos_saveneeded>"=="yes") {
         alert("Do not forget to activate the changes from the QoS [Interface Manager].");
       }
     }
  }
}
</script>
