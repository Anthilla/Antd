<xinclude menu_MouseEvent>

function manage() {
   top.body.location="/cgi-bin/kerbynet?Section=FW&STk=<xvar STk>&Action=Manage";
};
function ConnTrack() {
   top.body.location="/cgi-bin/kerbynet?Section=FW&STk=<xvar STk>&Action=ConnTrack";
};
function l7manage() {
   newwin=open("kerbynet?Section=FW&STk=<xvar STk>&Action=L7Manager", "L7Manager","top=250,left=250,width=650,height=450,scrollbars=yes,menubar=no,toolbar=no,statusbar=no");
   newwin.focus();
}
</script>
